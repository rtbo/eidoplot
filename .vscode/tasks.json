                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$rustc", "$rust-panic"],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            }
        }
    ]
