{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Sine example",
            "type": "shell",
            "command": "cargo run --example sine -- svg png",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$rustc", "$rust-panic"],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            }
        },
        {
            "label": "Gauss example",
