figure: {
    title: "Multiple axes"
    legend
    plot: {
        x-axis: "x", PiMultipleTicks
        y-axis: "sin(x)", Ticks
        y-axis: "exp(x)", Ticks, LogScale
        series: Line {
            name: "sin(x)"
            x-data: "x"
            y-data: "sin(x)"
        }
        series: Line {
            name: "exp(x)"
            x-data: "x"
            y-data: "exp(x)"
            y-axis: "exp(x)"
        }
    }
}
