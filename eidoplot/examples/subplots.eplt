figure: {
    title: "Subplots"
    space: 10
    subplots: 2, 1
    plot: {
        subplot: 1, 1
        x-axis: uses("x"), Grid
        y-axis: Ticks
        series: Line {
            x-data: "x1"
            y-data: "y1"
        }
    }
    plot: {
        subplot: 2, 1
        x-axis: PiMultipleTicks, Grid, id("x")
        y-axis: Ticks
        series: Line {
            x-data: "x2"
            y-data: "y2"
        }
    }
}
