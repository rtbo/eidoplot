figure: {
    title: "Subplots"
    space: 10
    subplots: 2, 1
    plot: {
        subplot: 1, 1
        x-axis: shared("x"), Grid
        y-axis: "y1", Ticks
        series: Line {
            x-data: "x1"
            y-data: "y1"
        }
    }
    plot: {
        subplot: 2, 1
        x-axis: "x", PiMultipleTicks, Grid, id("x-axis")
        y-axis: "y2", Ticks
        series: Line {
            x-data: "x2"
            y-data: "y2"
        }
    }
}
